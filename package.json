{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for react applications",
  "keywords": [],
  "author": {
    "name": "Max Zinchecnko",
    "url": "https://github.com/maxzinchenko"
  },
  "main": "server.js",
  "scripts": {
    "start:dev": "webpack-dev-server --config config/webpack.config.js --env development",
    "start:prod": "node server.js",
    "build": "rimraf dist && webpack --config config/webpack.config.js --env production",
    "build:start": "yarn build && node server.js",
    "webpack": "webpack",
    "lint:check": "eslint . --fix",
    "precommit": "yarn lint:check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "license": "ISC",
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@svgr/webpack": "5.4.0",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "css-loader": "4.3.0",
    "dotenv-webpack": "3.0.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-unicorn": "22.0.0",
    "file-loader": "6.1.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "sass-loader": "10.0.3",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "4.41.4",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  }
